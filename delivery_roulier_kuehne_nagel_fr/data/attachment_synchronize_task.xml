<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">

        <record id="kuehne_import_pl_task" model="attachment.synchronize.task">
            <field name="name">Kuehne Nagel import Plan de livraison</field>
            <field name="method_type">import</field>
            <field name="filepath">outbound</field>
            <field name="pattern">*.csv</field>
            <field name="backend_id" ref="kuehne_external_storage" />
            <field name="move_path">outbound/archives</field>
            <field name="after_import">move</field>
            <field name="file_type">import_directional_code</field>
        </record>

        <record id="kuehne_export_deposit_task" model="attachment.synchronize.task">
            <field name="name">Kuehne Nagel export deposit slip</field>
            <field name="method_type">export</field>
            <field name="filepath">inbound</field>
            <field name="backend_id" ref="kuehne_external_storage" />
        </record>
</odoo>
