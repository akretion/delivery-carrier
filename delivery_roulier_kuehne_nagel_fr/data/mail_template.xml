<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">

<record id="missing_directional_code_template" model="mail.template">
    <field eval="0" name="auto_delete" />
    <field name="email_from">admin@example.com</field>
    <field name="subject">[Order {{object.name}} Missing directionnal code</field>
    <field eval="[(6,0,[])]" name="attachment_ids" />
    <field name="model_id" ref="sale.model_sale_order" />
    <field name="email_to" />
    <field name="lang">{{object.partner_id.lang}}</field>
    <field name="name">Missing directionnal code</field>
    <field name="model">sale.order</field>
    <field name="body_html" type="html">
<div>
The sale order <t
                    t-esc="object.name"
                /> has no directional code for Kuehne Nagel delivery
Please select one before starting the shipping.
</div>
    </field>
</record>

</odoo>
